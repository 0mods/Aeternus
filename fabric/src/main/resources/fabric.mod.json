{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  
  "name": "${modName}",
  "description": "${description}",
  "authors": [
    "${modAuthor}",
    "${credits}"
  ],
  "contact": {},
  "license": "${license}",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "team.zeds.aeternus.fabric.AeternusFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "team.zeds.aeternus.fabric.client.AFClient"
      }
    ]
  },
  "mixins": [
    "${modId}.mixins.json",
    "${modId}.fabric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabricLoaderVersion}",
    "cloth-config": ">=${clothVersion}",
    "fabric": "*",
    "minecraft": "${minecraftVersion}",
    "java": ">=17",
    "fabric-language-kotlin": ">=${klfVersion}"
  },
  "accessWidener": "${modId}.accesswidener"
}
  